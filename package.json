{
  "name": "ng-boilerplate",
  "version": "1.0.0",
  "engines": {
    "npm": "10.8.1",
    "node": "v22.4.1"
  },
  "scripts": {
    "build": "ng build --localize",
    "cypress:open": "cypress open",
    "cypress:run:e2e": "ng e2e --headless",
    "cypress:run:component": "cypress run --component",
    "i18n": "ng extract-i18n --format=json --output-path=src/locale",
    "lint": "ng lint --max-warnings 0",
    "ng": "ng",
    "start": "ng serve",
    "test:all": "npm run test:component && npm run test:e2e",
    "test:clean:start": "npm run test:clean && shx rm -rf .nyc_output_data/*",
    "test:clean": "shx rm -rf coverage/* && shx rm -rf .nyc_output/*",
    "test:component:data": "nyc report --reporter=json --report-dir=./.nyc_output_data/component",
    "test:component": "tsc && nyc --silent cypress run --component && npm run test:component:data",
    "test:coverage": "nyc check-coverage --lines=100 --branches=100 --statements=100 --functions=100",
    "test:data:merge": "npm run test:data:move && npm run test:data:scrub && nyc merge .nyc_output_data .nyc_output/out.json",
    "test:data:move:component": "shx mv .nyc_output_data/component/coverage-final.json .nyc_output_data/component.json",
    "test:data:move:e2e": "shx mv .nyc_output_data/e2e/coverage-final.json .nyc_output_data/e2e.json",
    "test:data:move": "npm run test:data:move:component && npm run test:data:move:e2e",
    "test:data:scrub": "node nyc.util.mjs",
    "test:e2e:data": "nyc report --reporter=json --report-dir=./.nyc_output_data/e2e/",
    "test:e2e": "tsc && nyc --silent ng e2e --headless --watch=false && npm run test:e2e:data",
    "test:report": "nyc report",
    "test": "npm run test:clean:start && npm run test:all && npm run test:clean && npm run test:data:merge && npm run test:report && npm run test:coverage",
    "watch": "ng build --watch --configuration development"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "18.1.1",
    "@angular/common": "18.1.1",
    "@angular/compiler": "18.1.1",
    "@angular/core": "18.1.1",
    "@angular/forms": "18.1.1",
    "@angular/localize": "18.1.1",
    "@angular/platform-browser": "18.1.1",
    "@angular/platform-browser-dynamic": "18.1.1",
    "@angular/platform-server": "18.1.1",
    "@angular/router": "18.1.1",
    "@angular/ssr": "18.1.1",
    "@fortawesome/angular-fontawesome": "0.15.0",
    "@fortawesome/fontawesome-svg-core": "6.5.2",
    "@fortawesome/free-solid-svg-icons": "6.5.2",
    "@types/bootstrap": "5.2.10",
    "bootstrap": "5.3.3",
    "express": "4.19.2",
    "rxjs": "7.8.1",
    "tslib": "2.6.3",
    "zone.js": "0.14.8"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "18.0.0",
    "@angular-devkit/build-angular": "18.1.1",
    "@angular-eslint/builder": "18.1.0",
    "@angular-eslint/eslint-plugin": "18.1.0",
    "@angular-eslint/eslint-plugin-template": "18.1.0",
    "@angular-eslint/schematics": "18.1.0",
    "@angular-eslint/template-parser": "18.1.0",
    "@angular/cli": "18.1.1",
    "@angular/compiler-cli": "18.1.1",
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@cypress/code-coverage": "3.12.42",
    "@cypress/schematic": "2.5.2",
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@jsdevtools/coverage-istanbul-loader": "3.0.5",
    "@types/express": "4.17.21",
    "@types/jsonpath": "0.2.4",
    "@types/node": "20.14.12",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "angular-eslint": "18.1.0",
    "axe-core": "4.9.1",
    "cypress": "13.13.1",
    "cypress-axe": "1.5.0",
    "cypress-real-events": "1.13.0",
    "eslint": "8.57.0",
    "istanbul-lib-coverage": "3.2.2",
    "jsonpath": "^1.1.1",
    "nyc": "17.0.0",
    "path": "0.12.7",
    "shx": "0.3.4",
    "source-map-support": "0.5.21",
    "ts-node": "10.9.2",
    "typescript": "5.5.3",
    "typescript-eslint": "8.0.0-alpha.38"
  }
}
